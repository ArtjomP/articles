<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <Version>1.0</Version>

    <PackageId>pt.CommandExecutor.Common</PackageId>
    <Authors>ArtjomP</Authors>
    <Company>Panteam</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIconUrl>https://cdn0.iconfinder.com/data/icons/media-retro/60/001_-_play-1024.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/ArtjomP/articles/tree/main/command-executor/pt.CommandExecutor/pt.CommandExecutor.Common</RepositoryUrl>
    <Title>Run commands in a testable way</Title>
    <Description>https://github.com/ArtjomP/articles/blob/command-executor/command-executor/index.en.md</Description>
    <PackageTags>Command;Command Executor;</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Toolkit.Diagnostics" Version="7.1.2" />
  </ItemGroup>

</Project>
